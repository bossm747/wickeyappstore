    "@angular-devkit/build-ng-packagr": "~0.7.0-beta.1",
    "@angular/cli": "6.0.7",
    "@angular/language-service": "^6.0.3",
    "@compodoc/compodoc": "^1.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concat": "^1.0.3",
    "fs-extra": "^6.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
